{
    "name": "ttbooking/laravel-nanoid",
    "description": "Nano ID integration for Laravel.",
    "type": "library",
    "keywords": [
        "nanoid",
        "laravel",
        "laravel-nanoid"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ttbooking/laravel-nanoid/issues",
        "source": "https://github.com/ttbooking/laravel-nanoid"
    },
    "authors": [
        {
            "name": "Sergey Danilchenko",
            "email": "s.danilchenko@ttbooking.ru"
        }
    ],
    "require": {
        "php": "^8.0",
        "hidehalo/nanoid-php": "^1.1",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.7",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5.10 || ^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "TTBooking\\Nanoid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TTBooking\\Nanoid\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TTBooking\\Nanoid\\NanoidServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
